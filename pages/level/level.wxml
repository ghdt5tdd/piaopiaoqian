<!--pages/goodsdetail/goodsdetail.wxml-->
<view class='level-head'>
  <view class='level-head-left'>
    <view>承运商累计服务:<text>{{levelTotal}}</text>
    </view>
    <image class='{{levelNum}}' mode='aspectFit' src='{{levelPic}}'></image>
  </view>
  <view>服务率:<text>{{levelRate}}</text>
  </view>
</view>


<scroll-view scroll-x="true">
  <view class='goodsTable' style='width:{{goodsWidth}}'>
    <view class="table">
      <view class="tr">
        <view class='th' wx:for="{{tableTh}}">
          {{item.title}}
        </view>
      </view>
      <view class="tr" wx:for="{{tableTr}}">
        <view class='td' wx:for="{{item.td}}">
          {{item.name}}
        </view>
      </view>
    </view>
  </view>
</scroll-view>